<?xml version="1.0"?>
<launch>
  <!-- <arg name="limited" default="false"/> -->
  <arg name="paused" default="false"/>
  <arg name="gui" default="true"/>
  <arg name="mav_name" default="reed_quad_x"/>
  
  <!-- startup simulated world -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" default="worlds/empty.world"/>
    <arg name="paused" value="$(arg paused)"/>
    <arg name="gui" value="$(arg gui)"/>
  </include>

  <!-- send robot urdf to param server -->
  <param  name="robot_description"  textfile="$(find wy_qua_arm)/urdf/qua_arm_base.urdf" />  


  <!-- push robot_description to factory and spawn robot in gazebo -->
  <node name="spawn_gazebo_model" pkg="gazebo_ros" type="spawn_model" args="-urdf -param robot_description -model qua_arm" output="screen" />
 <include file="$(find le_arm)/launch/controller_utils.launch"/>

